{
  "name": "sergicontre-personal-blog",
  "version": "1.0.0",
  "description": "My personal blog with the Eleventy static site generator.",
  "scripts": {
    "build": "npm run build:wc && eleventy",
    "watch": "eleventy --watch",
    "serve": "npm run build:wc && eleventy --serve",
    "debug": "DEBUG=* eleventy",
    "build:wc": "rimraf _site/js && rollup -c rollup.config.js"
  },
  "author": {
    "name": "Sergio Contreras",
    "email": "sergicontre@gmail.com",
    "url": "https://sergicontre.dev/"
  },
  "license": "MIT",
  "devDependencies": {
    "@11ty/eleventy": "^0.10.0",
    "@11ty/eleventy-navigation": "^0.1.3",
    "@11ty/eleventy-plugin-rss": "^1.0.7",
    "@11ty/eleventy-plugin-syntaxhighlight": "^3.0.1",
    "luxon": "^1.21.3",
    "markdown-it": ">=12.3.2",
    "markdown-it-anchor": "^5.2.5",
    "babel": "^6.23.0",
    "@babel/core": "^7.10.1",
    "@babel/preset-env": "^7.10.1",
    "rollup": "^2.11.2",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-copy": "^3.3.0",
    "rollup-plugin-filesize": "^9.0.0",
    "rollup-plugin-minify-html-literals": "^1.2.4",
    "rollup-plugin-terser": "^6.1.0",
    "@rollup/plugin-node-resolve": "^8.0.0",
    "rimraf": "^3.0.2",
    "systemjs": "^6.3.2"
  },
  "dependencies": {
    "eleventy-plugin-reading-time": "0.0.1",
    "dark-mode-toggle": "^0.7.3"
